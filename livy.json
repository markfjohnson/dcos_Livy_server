{
  "id": "/livy-spark4",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 8998,
        "hostPort": 8998,
        "labels": {
          "VIP_0": "/livy-spark4:8998"
        },
        "protocol": "tcp",
        "servicePort": 10007,
        "name": "livy"
      },
      {
        "containerPort": 4040,
        "hostPort": 4040,
        "protocol": "tcp",
        "servicePort": 10008,
        "name": "sparkui"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "markfjohnson/livy4",
      "forcePullImage": true,
      "privileged": true,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 123,
      "delaySeconds": 15,
      "protocol": "HTTP"
    }
  ],
  "instances": 1,
  "labels": {
    "DCOS_SERVICE_NAME": "livy-spark4",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_SERVICE_SCHEME": "http",
    "HAPROXY_GROUP": "external"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 1024,
  "gpus": 0,
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": [],
  "cmd": "sed -i.bak s/0.0.0.0:7077/spark-dispatcher-hdfs-eventlog.marathon.l4lb.thisdcos.directory:7077/g  /apps/incubator-livy-0.4.0-incubating/conf/livy.conf  && sed -i.bak s/INFO/DEBUG/g /apps/incubator-livy-0.4.0-incubating/conf/log4j.properties && ./apps/incubator-livy-0.4.0-incubating/bin/livy-server"
}