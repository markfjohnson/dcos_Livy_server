{
  "id": "/livy",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "/opt/spark/dist/sbin/start-mesos-dispatcher.sh --master mesos://zk://zk-1.zk:2181  && /apps/build/livy/bin/livy-server",
  "container": {
    "portMappings": [
      {
        "containerPort": 8998,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/livycdh:8998"
        },
        "protocol": "tcp",
        "servicePort": 10000,
        "name": "livy"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "markfjohnson/livy:0.50",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 4040,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": [],
  "env": {
    "LIBPROCESS_IP": "0.0.0.0",
    "LIBPROCESS_PORT": "7228",
    "SPARK_MESOS_DISPATCHER_HOST": "0.0.0.0.0"
  }
}