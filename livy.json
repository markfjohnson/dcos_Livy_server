{
  "id": "/livy4-spark",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "type": "MESOS",
    "volumes": [],
    "docker": {
      "image": "markfjohnson/livy:0.40",
      "forcePullImage": true,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "env": {
    "SPARK_HOME": "/opt/spark/dist",
    "PATH": "/usr/bin:/bin:/opt/spark/dist/bin",
    "HADOOP_CONF_DIR":"/etc/hadoop/conf"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "port": 8998,
      "timeoutSeconds": 123,
      "delaySeconds": 15,
      "protocol": "MESOS_HTTP"
    }
  ],
  "instances": 1,
  "labels": {
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_NAME": "livy4-spark",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "HAPROXY_GROUP": "external"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 1024,
  "gpus": 0,
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}